AAA,KAAA;AACA;AA/BA,CAAA","sourcesContent":["<template>\n  <div>\n    <h1>广告位列表</h1>\n    <el-table :data=\"items\">\n      <el-table-column prop=\"_id\" label=\"ID\" width=\"230\"></el-table-column>\n      <!-- 后端接口添加了populate方法，把parent变成了一个对象，以此来获取它的name。否则prop=parent只能拿到它的id -->\n      <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"180\">\n        <template slot-scope=\"scope\">\n          <!-- scope表示当前行 -->\n          <el-button @click=\"$router.push(`/ads/edit/${scope.row._id}`)\" type=\"text\" size=\"small\">编辑</el-button>\n          <el-button @click=\"remove(scope.row)\" type=\"text\" size=\"small\">删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      items: []\n    }\n  },\n  methods: {\n    //重新调用\n    async fetch () {\n      const res = await this.$http.get('/rest/ads')\n      this.items = res.data\n    },\n    async remove(row){\n      this.$confirm(`是否要删除分类${row.name}`, '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(async () => {\n          //注意这边是row_id\n          const res = await this.$http.delete(`/rest/ads/${row._id}`)\n          this.items = res.data\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          this.fetch()\n        });\n    }\n  },\n  created () {\n    this.fetch()\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>"],"sourceRoot":"src/views"}]}